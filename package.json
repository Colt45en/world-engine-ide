{
  "author": "Your Name",
  "dependencies": {
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "express": "^4.17.1",
    "lucide-react": "^0.562.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.2.0",
    "ws": "^8.18.0"
  },
  "description": "An integrated development environment for a world engine setup focusing on mathematics and English, with an AI brain for internal application maintenance.",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@playwright/test": "^1.57.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.12.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^30.2.0",
    "chai": "^6.2.2",
    "cron": "^4.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.2.0",
    "hint": "^2.0.0",
    "jest": "^29.0.0",
    "lint-staged": "^15.5.2",
    "markdownlint-cli2": "^0.20.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.0",
    "prettier": "^3.2.5",
    "stylelint": "^15.0.0",
    "stylelint-config-standard": "^32.0.0",
    "supertest": "^6.0.0",
    "tailwindcss": "^3.4.17",
    "three": "^0.182.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1"
  },
  "keywords": [
    "IDE",
    "world engine",
    "mathematics",
    "English",
    "AI"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "name": "world-engine-ide",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,json,jsonc,css,scss,md,yml,yaml,html}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "vite build",
    "ci": "npm run format && npm run lint:md && npm run lint && npm run typecheck && npm run build",
    "dev": "vite",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,json,jsonc,md,yml,yaml,css,scss,html,cjs,mjs}\" \"server/**/*.{js,jsx,ts,tsx,json,jsonc,md,yml,yaml,css,scss,html,cjs,mjs}\" \"scripts/**/*.{js,jsx,ts,tsx,json,jsonc,md,yml,yaml,css,scss,html,cjs,mjs}\" \"docs/**/*.{md,yml,yaml}\" \".github/**/*.{md,yml,yaml,html}\" \"*.{js,jsx,ts,tsx,json,jsonc,md,yml,yaml,css,scss,html,cjs,mjs}\" --check",
    "format:write": "prettier \"src/**/*.{js,jsx,ts,tsx,json,jsonc,md,yml,yaml,css,scss,html,cjs,mjs}\" \"server/**/*.{js,jsx,ts,tsx,json,jsonc,md,yml,yaml,css,scss,html,cjs,mjs}\" \"scripts/**/*.{js,jsx,ts,tsx,json,jsonc,md,yml,yaml,css,scss,html,cjs,mjs}\" \"docs/**/*.{md,yml,yaml}\" \".github/**/*.{md,yml,yaml,html}\" \"*.{js,jsx,ts,tsx,json,jsonc,md,yml,yaml,css,scss,html,cjs,mjs}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"server/**/*.{js,ts}\" \"scripts/**/*.{js,ts}\" \"test/**/*.{js,ts}\" \"tests/**/*.{js,ts}\" \"vite.config.{js,ts}\" \"playwright.config.{js,ts}\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:md": "markdownlint-cli2 --config .markdownlint-cli2.jsonc",
    "preview": "vite preview",
    "brain:relay": "cross-env PYTHONPATH=brain/src RELAY_PORT=9001 node scripts/run-python.cjs -m brain.relay.ws_server",
    "serve:dist": "node scripts/run-python.cjs scripts/serve_dist.py",
    "serve:dist:open": "node scripts/run-python.cjs scripts/serve_dist.py --open",
    "server:full": "node scripts/full-server.cjs",
    "py:format": "python -m black brain",
    "py:format:check": "python -m black --check brain",
    "py:lint": "python -m ruff check brain",
    "py:test": "pytest -q brain/tests",
    "start": "node scripts/start.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "verify": "npm run ci && npm run py:lint && npm run py:format:check && npm run py:test",
    "webhint:feed:public": "node scripts/webhint/buildTree.js public/webhint-feed.json"
  },
  "version": "1.0.0"
}
